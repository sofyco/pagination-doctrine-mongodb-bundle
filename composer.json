{
    "name": "sofyco/pagination-doctrine-mongodb-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Pagination Doctrine MongoDB Bundle",
    "keywords": ["php", "symfony", "pagination", "doctrine", "mongodb", "odm", "bundle"],
    "homepage": "https://github.com/sofyco/pagination-doctrine-mongodb-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiia Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-mongodb": "*",
        "doctrine/mongodb-odm-bundle": "^5.0",
        "sofyco/pagination-bundle": "^2.0",
        "symfony/framework-bundle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\Pagination\\Doctrine\\MongoDB\\AdapterBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\Pagination\\Doctrine\\MongoDB\\AdapterBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
